{
	"name": "@retellai/n8n-nodes-retellai",
	"version": "0.1.1",
	"description": "Official n8n node for RetellAI API integration",
	"keywords": [
		"n8n-community-node-package",
		"n8n-node-retellai",
		"retellai",
		"conversational ai",
		"voice ai"
	],
	"license": "MIT",
	"homepage": "https://retellai.com",
	"author": {
        "name": "RetellAI",
        "email": "support@retell.ai"
    },
	"repository": {
		"type": "git",
		"url": "git+https://github.com/RetellAI/n8n-nodes-retellai.git"
	},
	"main": "index.js",
	"scripts": {
		"build": "tsc && gulp build:icons",
		"dev": "tsc --watch",
		"format": "prettier nodes credentials --write",
		"lint": "eslint nodes credentials package.json",
		"lintfix": "eslint nodes credentials package.json --fix",
		"prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/RetellAIApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/RetellAI/RetellAI.node.js"
		]
	},
	"devDependencies": {
		"@types/express": "^4.17.17",
		"@types/request-promise-native": "~1.0.18",
		"@typescript-eslint/parser": "~5.45",
		"eslint-plugin-n8n-nodes-base": "^1.16.0",
		"gulp": "^4.0.2",
		"n8n-workflow": "^1.5.0",
		"prettier": "^2.7.1",
		"typescript": "~4.8.4"
	},
	"dependencies": {
		"ws": "^8.16.0"
	},
	"bugs": {
		"url": "https://github.com/RetellAI/n8n-nodes-retellai/issues"
	}
}
